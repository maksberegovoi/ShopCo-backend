        "target": "ES2023",
        "module": "commonjs",
        "rootDir": "src",
        "outDir": "dist",

        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,

        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src"
    ]
}
